{
  "name": "gardenz",
  "version": "0.1.4",
  "main": "server/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harakeke-2021/gardenz.git"
  },
  "scripts": {
    "start": "node server",
    "build": "run-p build:client build:server",
    "build:client": "vite build",
    "build:server": "npm run db:migrate",
    "dev": "run-p dev:client dev:server dev:css",
    "dev:client": "vite",
    "dev:server": "nodemon server/index.js",
    "dev:css": "tailwindcss -i client/css/main.css -o server/public/main.css --watch",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:reset": "npm run knex migrate:rollback && npm run db:migrate && npm run db:seed",
    "postinstall": "run-s db:migrate db:seed",
    "test": "jest --selectProjects client server",
    "test:e2e": "jest --selectProjects e2e --runInBand",
    "test:watch": "npm run test -- --watchAll",
    "lint": "eslint --ext .js,.jsx .",
    "vite:start": "vite",
    "vite:build": "vite build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "eda/react",
    "ignorePatterns": [
      "bundle.js"
    ],
    "rules": {
        "no-console": "error"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "eslint.validate": [
    {
      "language": "vue",
      "autoFix": true
    },
    {
      "language": "html",
      "autoFix": true
    },
    {
      "language": "javascript",
      "autoFix": true
    },
    {
      "language": "typescript",
      "autoFix": true
    }
  ],
  "jest": {
    "verbose": true,
    "projects": [
      {
        "displayName": "client",
        "testEnvironment": "jsdom",
        "testMatch": [
          "<rootDir>/client/**/*.test.js"
        ],
        "setupFilesAfterEnv": [
          "@testing-library/jest-dom"
        ]
      },
      {
        "displayName": "server",
        "testMatch": [
          "<rootDir>/server/**/*.test.js"
        ]
      },
      {
        "displayName": "e2e",
        "testMatch": [
          "**/e2e/?(*.)+(spec|test).[jt]s?(x)"
        ]
      }
    ]
  },
  "engines": {
    "node": "14.16",
    "npm": "6.14.11"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.5.0",
    "@reduxjs/toolkit": "^1.8.3",
    "@visx/axis": "^2.1.1",
    "@visx/gradient": "^2.1.0",
    "@visx/group": "^2.1.0",
    "@visx/scale": "^2.1.0",
    "@visx/shape": "^2.1.1",
    "@visx/visx": "^2.1.1",
    "axios": "^0.21.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "knex": "^1.0.3",
    "limiter": "^2.1.0",
    "moment": "^2.29.1",
    "pg": "^8.6.0",
    "react": "^17.0.2",
    "react-collapsible": "^2.8.4",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-leaflet": "^3.2.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^6.2.1",
    "superagent": "^6.1.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-vite": "^0.1.37",
    "@storybook/react": "^6.1.21",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@vitejs/plugin-react": "^1.3.2",
    "@vscode/sqlite3": "^5.0.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "eslint": "^8.10.0",
    "eslint-config-eda": "^1.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "jest": "^27.0.6",
    "jest-playwright-preset": "^1.6.1",
    "leaflet": "^1.7.1",
    "nock": "^13.1.3",
    "node-jsonwebtoken": "0.0.1",
    "nodemon": "^2.0.18",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.12.3",
    "prettier": "^2.5.1",
    "regenerator-runtime": "^0.13.7",
    "supertest": "^6.1.3",
    "tailwindcss": "^3.1.4",
    "vite": "^2.9.14"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  }
}
